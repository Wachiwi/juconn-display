{
  "name": "resin-electronjs",
  "version": "0.2.0",
  "description": "electronJS-based resin application",
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "dev": "react-scripts start & URL_LAUNCHER_NODE=1 URL_LAUNCHER_URL=http://localhost:3000 NODE_ENV=development electron .",
    "lint": "eslint .",
    "start": "URL_LAUNCHER_NODE=1 NODE_ENV=production startx ./node_modules/electron/dist/electron .",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "URL_LAUNCHER_NODE=1 electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resin-io/resin-electronjs.git"
  },
  "keywords": [
    "resin",
    "io",
    "electron",
    "js"
  ],
  "dependencies": {
    "electron": "^2.0.0",
    "electron-rebuild": "^1.7.3",
    "react": "16.3.2",
    "react-dom": "16.3.2"
  },
  "author": "Carlo Maria Curinga",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/resin-io/resin-electronjs/issues"
  },
  "jshintConfig": {
    "esversion": 6,
    "strict": true
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "react-scripts": "1.1.4"
  }
}
