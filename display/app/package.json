{
  "name": "smarthome-display",
  "version": "0.2.0",
  "description": "electronJS-based resin application",
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "dev": "concurrently \"react-scripts start\" \"URL_LAUNCHER_NODE=1 URL_LAUNCHER_URL=http://localhost:3000 NODE_ENV=development electron .\"",
    "lint": "eslint .",
    "start": "URL_LAUNCHER_NODE=1 NODE_ENV=production startx ./node_modules/electron/dist/electron .",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "URL_LAUNCHER_NODE=1 electron .",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql --extensions js jsx"
  },
  "dependencies": {
    "@material-ui/core": "^1.0.0-rc.1",
    "electron": "^2.0.0",
    "electron-rebuild": "^1.7.3",
    "node-sass": "^4.9.0",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-motion": "^0.5.2",
    "react-relay": "^1.6.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-transition": "^1.2.1",
    "resin-sdk": "^8.2.1"
  },
  "jshintConfig": {
    "esversion": 6,
    "strict": true
  },
  "devDependencies": {
    "babel-plugin-relay": "^1.6.0",
    "concurrently": "^3.5.1",
    "electron-prebuilt-compile": "^2.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "graphql": "^0.13.2",
    "react-scripts": "1.1.4",
    "relay-compiler": "^1.6.0"
  }
}
